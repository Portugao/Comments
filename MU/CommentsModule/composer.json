{
    "name": "mu/comments-module",
    "version": "1.0.0",
    "description": "MUCommentsModule module generated by ModuleStudio 1.0.1.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\CommentsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "1.*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.0 <3.0",
            "class": "MU\\CommentsModule\\MUCommentsModule",
            "displayname": "Comments",
            "url": "comments",
            "capabilities": {
                "user": {"route": "mucommentsmodule_comment_index"},
                "admin": {"route": "mucommentsmodule_comment_adminindex"}
            },
            "securityschema": {
                "MUCommentsModule::": "::",
                "MUCommentsModule:ItemListBlock:": "Block title::",
                "MUCommentsModule:ModerationBlock:": "Block title::",
                "MUCommentsModule:Comment:": "Comment ID::",
                "MUCommentsModule:Comment:Comment": "Comment ID:Comment ID:",
                "MUCommentsModule::Ajax": "::"
            }
        }
    }
}
