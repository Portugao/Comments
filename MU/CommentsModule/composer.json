{
    "name": "mu/comments-module",
    "version": "1.0.0",
    "description": "MUCommentsModule generated by ModuleStudio 1.4.0.",
    "type": "zikula-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\CommentsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^1",
        "pdepend/pdepend": "^2",
        "phploc/phploc": "^4",
        "phpmd/phpmd": "^2",
        "phpunit/phpunit": "^5",
        "sebastian/phpcpd": "^3",
        "squizlabs/php_codesniffer": "^3",
        "theseer/phpdox": "^0"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=2.0.11 <3.0.0",
            "class": "MU\\CommentsModule\\MUCommentsModule",
            "displayname": "Comments",
            "url": "comments",
            "capabilities": {
                "user": {"route": "mucommentsmodule_comment_index"},
                "admin": {"route": "mucommentsmodule_comment_adminindex"}
            },
            "securityschema": {
                "MUCommentsModule::": "::",
                "MUCommentsModule:ItemListBlock:": "Block title::",
                "MUCommentsModule:ItemBlock:": "Block title::",
                "MUCommentsModule:ModerationBlock:": "Block title::",
                "MUCommentsModule:Comment:": "Comment ID::",
                "MUCommentsModule:Comment:Comment": "Comment ID:Comment ID:",
                "MUCommentsModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0.30"
        },
        "sort-packages": true
    }
}
